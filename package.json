{
	"name": "@agado/fastify-pg-stream",
	"version": "1.0.0",
	"description": "A Fastify sse streaming example backed by pg listen/notify",
	"scripts": {
		"start": "pnpm --filter @agado/api-starter start",
		"build": "turbo build",
		"test": "turbo test",
		"test:coverage": "turbo test:coverage",
		"dev": "turbo dev",
		"build:contract": "turbo build --filter=@agado/api-starter-contract",
		"build:api": "turbo build --filter=@agado/api-starter",
		"db:migrate": "pnpm --filter @agado/api-starter prisma:migrate",
		"check": "turbo run check",
		"ci": "pnpm build && pnpm test:coverage && pnpm check"
	},
	"keywords": [
		"fastify",
		"prisma",
		"typescript",
		"auth0",
		"openapi",
		"ts-rest",
		"turbo",
		"sentry"
	],
	"devDependencies": {
		"turbo": "^2.4.2"
	},
	"packageManager": "pnpm@10.3.0"
}
